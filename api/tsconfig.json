{
  "compilerOptions": {
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "types": ["./src/cloudflare.d.ts", "node"]
  },
  "exclude": ["node_modules"],
  "include": ["src", "../data-generator/schema.ts"]
}